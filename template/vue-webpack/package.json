{
  "name": "@template/vue-webpack",
  "version": "0.1.0",
  "description": "Vue project template using webpack",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config webpack.conf.js --open",
    "watch": "webpack --config webpack.conf.js --watch",
    "build": "webpack --config webpack.conf.js --mode production",
    "dev:build": "webpack --config webpack.conf.js --mode development",
    "eslint:check": "eslint --ext .js,.ts,.vue src/",
    "eslint:fix": "eslint --fix --ext .js,.ts,.vue src/",
    "test": "jest"
  },
  "author": "SystemLight",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^6.2.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^4.0.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  }
}
